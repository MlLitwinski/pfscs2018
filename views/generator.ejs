                        <input type="checkbox" id="pj_kat_b1" class="ch_pj">
                        <label for="pj_kat_b1" class="cursor"><span></span><i>B1</i></label>
                        </div>
                        <div>
                        <input type="checkbox" id="pj_kat_c" class="ch_pj">
                        <label for="pj_kat_c" class="cursor"><span></span><i>C</i></label>
                        </div>
                        <div>
                        <input type="checkbox" id="pj_kat_c1" class="ch_pj">
                        <label for="pj_kat_c1" class="cursor"><span></span><i>C1</i></label>
                        </div>
                        <div>
                        <input type="checkbox" id="pj_kat_d" class="ch_pj">
                        <label for="pj_kat_d" class="cursor"><span></span><i>D</i></label>
                        </div>
                        <div>
                        <input type="checkbox" id="pj_kat_d1" class="ch_pj">
                        <label for="pj_kat_d1" class="cursor"><span></span><i>D1</i></label>
                        </div>
                        <div>
